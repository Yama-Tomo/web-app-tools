{
  "name": "@yamatomo/panda-preset",
  "version": "0.1.0",
  "description": "Panda CSS preset",
  "keywords": [],
  "license": "MIT",
  "author": "Yama-Tomo",
  "files": [
    "./dist",
    "./README.md"
  ],
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "main": "./dist/index.cjs",
  "imports": {
    "#sb/*": "./.storybook/*",
    "#panda/*": {
      "types": "./styled-system/*/index.d.ts",
      "default": "./styled-system/*/index.mjs"
    },
    "#utils": "./src/utils/index.ts",
    "#recipes/*": "./src/recipes/*/index.ts",
    "#*": "./src/*"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "prepare": "panda codegen",
    "build": "tsdown --format esm --format cjs",
    "gh-publish": "gh-publish",
    "typecheck": "tsc",
    "storybook": "storybook dev",
    "build-storybook": "storybook build"
  },
  "engines": {
    "node": ">=22.4.0"
  },
  "peerDependencies": {
    "@pandacss/dev": "^1.8.2"
  },
  "devDependencies": {
    "@base-ui/react": "^1.2.0",
    "@storybook/addon-themes": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@yamatomo/gh-publish": "workspace:*",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "storybook": "^10.2.8"
  }
}
