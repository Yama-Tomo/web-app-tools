{
  "name": "@yamatomo/react-router-hono-bun-example",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "./scripts/install-workspace-packages",
    "build": "bunx --bun react-router build",
    "dev": "bunx --bun react-router dev",
    "start": "NODE_ENV=production ./build/server/server.js",
    "typecheck": "react-router typegen && tsc",
    "test": "pnpm '/test:.*/'",
    "test:dev": "TEST_TARGET=dev playwright test",
    "test:prod": "pnpm build && playwright test"
  },
  "dependencies": {
    "@react-router/node": "^7.12.0",
    "@react-router/serve": "^7.12.0",
    "isbot": "^5.1.31",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router": "^7.12.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@react-router/dev": "^7.12.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/bun": "^1.3.6",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.1.9",
    "msw": "^2.12.7",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.2",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.4"
  },
  "patchedDependencies": {
    "vite@7.2.2": "patches/vite@7.2.2.patch"
  }
}
